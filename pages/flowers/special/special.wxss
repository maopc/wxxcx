.special-container{
    border-top: solid 1px #aaa; 
    background: #eee;
    width: 100%;
    height: 100%;
}
.s-list{
    display: flex;
    flex-wrap: wrap;
}
.s-list navigator {
  box-sizing: border-box;
  width: 100%;
}
.s-list li{
    position: relative;
    box-sizing: border-box;
    width: 100%;
}
.s-list li image{
    width: 100%;
    height: 180px;
    filter: Brightness(0.7);
    float: left;
}
.s-list li b{
    position: absolute;
    left: 0;
    top: 40%;
    width:100%;
    display: block;
    text-align: center;
    color: #fff;
    font-size: 40rpx;
}
.s-list li i{
    position: absolute;
    left: 0;
    top: 50%;
    width:100%;
    display: block;
    text-align: center;
    color: #fff;
    font-size: 16rpx;
    padding-top: 5rpx;
}
.special-container scroll-view {
    height: 100%;
    width: 100%;
    overflow: hidden;
}
.special-container scroll-view button {
  font-size: 14px;
  border: 0;
}
